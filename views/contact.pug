<body>
    <div class="maincontainer">
        <nav class="navbar">
            <a href="/">Home</a>
            <a href="/">Services</a>
            <a href="/">Sponcers</a>
            <a href="/contact">Contact</a>

        </nav>

        <h2 class="contactheading">Contact us </h2>
        <form class="formsubmition" action="contact" method="post">
            <input type="text" placeholder="Enter your First Name" required name="firstname">
            <input type="text" placeholder="Enter your Last Name" required name="lastname">

            <input type="email" placeholder="Enter your Email" required name="email">
            <input type="text" placeholder="Enter your Mobile number" name="phone">
            <input type="text" placeholder="Enter Your City" required name="city">

            <label for="type">Select category:</label>
            <select name="type" id="cars">
            <option value="Hip Hop">Hip Hop</option>
            <option value="Jazz">Jazz</option>
            <option value="Folk">Folk</option>
            </select>
            <input class="submitbtn" type="submit" placeholder="Submit Now"> 
        </form>
    </div>
